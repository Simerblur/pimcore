{% extends 'layout/base.html.twig' %}
{% block title %}{{'_PROJECTEWPA_META_TITLE_' | trans}}{% endblock %}
{% block keywords %}{{'_PROJECTEWPA_META_KEYWORDS_' | trans}}{% endblock %}
{% block description %}{{'_PROJECTEWPA_META_DESCRIPTION_' | trans}}{% endblock %}
{% block body %}
    <div id="ewpa" class="main-content-wrapper jsScrollToSection">
        <!-- section top  -->
        <section class="top-single-project-section">
            <div class="content-container-static">
                <h5 class="category-headers" data-aos="fade-right" data-aos-duration="500" data-aos-delay="300">{{'_PROJECT_HEADER_CATEGORY_MAIN_' | trans}}</h5>
                <h3 class="title-single-project" data-aos="fade-right" data-aos-duration="500" data-aos-delay="600">{{'_EWPA_PROJECT_MAIN_TITLE_' | trans}}</h3>
                <div class="info-description-floats" data-aos="fade-right" data-aos-duration="500" data-aos-delay="900">
                    <div class="info-description-floats-left">
                        <h5 class="category-headers inf-url-scope">{{'_INFO_SINGLE_PROJECT_' | trans}}</h5>
                        <p class="main-paragraph">{{'_INFO_DESCRIPTION_SINGLE_PROJECT_EWPA_' | trans}}</p>
                    </div>
                    <div class="info-description-floats-right">
                        <div>
                            <h5 class="category-headers inf-url-scope">{{'_WEBSITE_SINGLE_PROJECT_' | trans}}:</h5>
                            <a href="http://ewpa.pl" target="_blank" rel="nofollow">ewpa.pl</a>
                        </div>
                        <div>
                            <h5 class="category-headers inf-url-scope">{{'_SCOPE_SINGLE_PROJECT_' | trans}}:</h5>
                            <p class="main-paragraph category-paragraph">Visual design, UX design, Development</p>
                        </div>
                    </div>
                    <div class="info-description-floats-right platform-image-container">
                        <div>
                            <h5 class="category-headers inf-url-scope">{{'_PLATFORM_' | trans}}:</h5>
                            <div class="platform-image">
                                <img src="/images/svg/magento_project_view.svg" alt="{{'_EWPA_PROJECT_IMAGE_0_ALT_' | trans}}"/>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="both-clear"></div>
                <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="1300" src="/images/projects/ewpa/traktor.png" id="traktor" alt="{{'_EWPA_PROJECT_IMAGE_1_ALT_' | trans}}">
                <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="1100" src="/images/projects/ewpa/imac.png" id="imac-ewpa" alt="{{'_EWPA_PROJECT_IMAGE_2_ALT_' | trans}}">
            </div>
        </section>
        <!-- top second section -->
        <section class="top-second-section">
            <div class="content-container-static">
                <h3>{{'_WE_MADE_CHANGES_' | trans}}</h3>
                <div id="new-old">
                    <img data-aos="fade-right" data-aos-duration="500" src="/images/projects/ewpa/macbook.png" alt="{{'_EWPA_PROJECT_IMAGE_3_ALT_' | trans}}">
                    <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="300" src="/images/projects/ewpa/notmac.png" alt="{{'_EWPA_PROJECT_IMAGE_4_ALT_' | trans}}">
                    <p id="newvsold">{{'_NEW_VS_OLD_' | trans}}</p>
                </div>
                <div class="both-clear"></div>
            </div>
        </section>
        <!-- responsive showcase -->
        <section class="responsive-showcase-section">
            <div class="content-container-static">
                <h3 data-aos="fade-right" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500"><span>{{'_NOT_ONLY_' | trans}}</span><br>{{'_AN_EXCAVATORS_' | trans}}</h3>
                <p class="main-paragraph" data-aos="fade-right" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="500">{{'_RESPONSIVE_SHOWCASE_DESCRIPTION_EWPA_' | trans}}</p>
                <img id="sites-view" src="/images/projects/ewpa/showcase.png" alt="{{'_EWPA_PROJECT_IMAGE_5_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="700">
            </div>
        </section>
        <!-- about the technology -->
        <section class="about-technology-section">
            <div class="content-container-static">
                <h3 data-aos="fade-up">{{'_ABOUT_THE_TECHNOLOGY_' | trans}}</h3>
                <div class="technology-items-container" data-aos="zoom-in" data-aos-duration="600" data-aos-delay="300">
                    <div class="it-item" id="magento">Magento</div> 
                    <div class="it-item" id="php7">php7</div>
                    <div class="it-item" id="jquery">jQuery</div>
                    <div class="it-item" id="js">JS</div>
                    <div class="it-item" id="mysql">MySql</div>
                    <div class="it-item" id="html5">HTML5</div>
                    <div class="it-item" id="css3">CSS3</div>
                </div>
                <div class="both-clear"></div>
            </div>
        </section>
        <!-- overwiev -->
        <section class="overview-section">
            <div class="content-container-static">
                <h3 data-aos="fade-up">{{'_A_LOOK_@_THE_' | trans}}<br><span>{{'_WHOLE_' | trans}}</span></h3>
                <p class="main-paragraph" id="desc-paragraph" data-aos="fade-up">{{'_OVERVIEW_DESCRIPTION_EWPA_' | trans}}</p>
                <div class="both-clear"></div>

                <div class="overview-description">
                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.01</h5>
                        <h6>{{'_MAIN_PAGE_' | trans}}<br><span>{{'_HOME_DIR' | trans}}</span></h6>
                    </div>
                    <div class="overview-right" data-aos="fade-left">
                        <img class="whole-site-image" src="/images/projects/ewpa/site.jpg" alt="{{'_EWPA_PROJECT_IMAGE_6_ALT_' | trans}}">
                    </div>
                    <div class="both-clear"></div>

                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.02</h5>
                        <h6>{{'_CATEGORY_' | trans}}<br><span>&amp; {{'_PRODUCT_PAGE_' | trans}}</span></h6>
                    </div>
                    <div class="overview-right">
                        <div class="cat-presentation">
                            <img src="/images/projects/ewpa/overview-1.jpg" alt="{{'_EWPA_PROJECT_IMAGE_7_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation">
                            <img src="/images/projects/ewpa/overview-2.jpg" alt="{{'_EWPA_PROJECT_IMAGE_8_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation" data-aos-delay="300">
                        </div>
                        <div class="both-clear"></div>
                    </div>

                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.03</h5>
                        <h6>{{'_ABOUT_US_' | trans}}<br><span>&amp; {{'_CONTACT_' | trans}}</span></h6>
                    </div>
                    <div class="overview-right">
                        <div class="cat-presentation second">
                            <img src="/images/projects/ewpa/overview-3.jpg" alt="{{'_EWPA_PROJECT_IMAGE_9_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation">
                            <img src="/images/projects/ewpa/overview-4.jpg" alt="{{'_EWPA_PROJECT_IMAGE_10_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation.sec" data-aos-delay="300">
                        </div>
                        <div class="both-clear"></div>
                    </div>
                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.04</h5>
                        <h6>{{'_NEWS_' | trans}}<br><span>&amp; {{'_SEE_THE_NEWS_' | trans}}</span></h6>
                    </div>
                    <div class="overview-right">
                        <div class="cat-presentation third">
                            <img src="/images/projects/ewpa/overview-5.jpg" alt="{{'_EWPA_PROJECT_IMAGE_11_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation.third">
                            <img src="/images/projects/ewpa/overview-6.jpg" alt="{{'_EWPA_PROJECT_IMAGE_12_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation" data-aos-delay="300">
                        </div>
                        <div class="both-clear"></div>
                    </div>
                    <div class="both-clear"></div>
                </div>
                <div class="both-clear"></div>  
            </div>
        </section>
        <!-- our team -->
        <section class="our-crazy-team">
            <div class="content-container-static">
                <h3 data-aos="fade-up">{{'_OUR_CRAZY_TEAM_' | trans}}</h3>
                <div id="project-manager-container" data-aos="fade-down" data-aos-duration="500" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/kornelia.png" alt="Kornelia" class="person-default">
                        <img src="/images/persons/custom-shapes/projectmanagerhover.png" alt="Kornelia" class="person-hover">
                        <div class="person-desc">
                            <h6>.01</h6>
                            <p>KORNELIA<br><span>{{'_PROJECT_MANAGER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
                <div id="ui-ux-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="200" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/iza.png" alt="Iza" class="person-default">
                        <img src="/images/persons/custom-shapes/uiuxhover.png" alt="Iza" class="person-hover">
                        <div class="person-desc">
                            <h6>.02</h6>
                            <p>IZA<br><span>{{'_UIUX_DESIGNER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
                <div id="graphic-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="400" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/tomekgraphic.png" alt="Tomek" class="person-default">
                        <img src="/images/persons/custom-shapes/graphicdesignerhover.png" alt="Tomek" class="person-hover">
                        <div class="person-desc">
                            <h6>.03</h6>
                            <p>TOMEK<br><span>{{'_DEVELOPER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
                <div id="front-end-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="600" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/blazejfront.png" alt="Błażej" class="person-default">
                        <img src="/images/persons/custom-shapes/frontendhover.png" alt="Błażej" class="person-hover">
                        <div class="person-desc">
                            <h6>.04</h6>
                            <p>BŁAŻEJ<br><span>{{'_DEVELOPER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
                <div id="back-end-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="800" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/piotrdeveloper.png" alt="Piotr" class="person-default">
                        <img src="/images/persons/custom-shapes/developerhover.png" alt="Piotr" class="person-hover">
                        <div class="person-desc">
                            <h6>.05</h6>
                            <p>PIOTR<br><span>{{'_DEVELOPER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- next/prev project -->
        <section class="next-prev-section">
            <div class="content-container-static">
                <a href="{{ path('easybathroomsproject') }}" id="prev-btn"><span></span><p>{{'_PREVIOUS_' | trans}}</p></a>
                <h6><a href="{{ path('projectslist') }}">{{'_SEE_ALL_PROJECTS_' | trans}}</a></h6>
                <a href="{{ path('orgranproject') }}" id="next-btn"><p>{{'_NEXT_' | trans}}</p><span></span></a>
            </div>
        </section>
    </div>
    <!-- animation on scroll scritp -->
    <script src="/scripts/aos/aos.js"></script>
    <script>
        AOS.init({
            easing: 'ease-out-back',
            once: true
        });
    </script>
    <script src="{{ asset('/scripts/Advox/scrollToSection.js') }}"></script>
{% endblock %}


