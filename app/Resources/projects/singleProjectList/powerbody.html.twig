            <img data-aos="fade-up" data-aos-anchor=".top-second-section" data-aos-duration="300" data-aos-delay="600" id="mac-more"{% if app.request.getLocale() == 'en' %} src="../images/projects/powerbody/iphone-more.png" {% else %} src="../images/projects/powerbody/iphone-more-pl.png" {% endif %} alt="{{'_PB_PROJECT_IMAGE_7_ALT_' | trans}}">
