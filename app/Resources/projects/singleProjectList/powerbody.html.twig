        <div class="content-container-static">
            <h3>{{'_POWERBODY_SUBTITLE_' | trans}}</h3>
            <img data-aos="fade-up" data-aos-anchor=".top-second-section" data-aos-duration="300" id="mac-magento" {% if app.request.getLocale() == 'en' %}src="../images/projects/powerbody/imac-magento.png"{% else %}src="../images/projects/powerbody/imac-magento-pl.png"{% endif %} alt="{{'_PB_PROJECT_IMAGE_4_ALT_' | trans}}">
            <img data-aos="fade-up" data-aos-anchor=".top-second-section" data-aos-duration="300" data-aos-delay="200" id="mac-erp" {% if app.request.getLocale() == 'en' %} src="../images/projects/powerbody/macbook-erp.png" {% else %} src="../images/projects/powerbody/macbook-erp-pl.png" {% endif %}  alt="{{'_PB_PROJECT_IMAGE_5_ALT_' | trans}}">
