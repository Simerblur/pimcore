{% extends 'layout/base.html.twig' %}
{% block title %}{{'_PROJECTPOWERBODY_META_TITLE_' | trans}}{% endblock %}
{% block keywords %}{{'_PROJECTPOWERBODY_META_KEYWORDS_' | trans}}{% endblock %}
{% block description %}{{'_PROJECTPOWERBODY_META_DESCRIPTION_' | trans}}{% endblock %}
{% block body %}
<div id="powerbody" class="main-content-wrapper jsScrollToSection">
    <!-- section top  -->
    <section class="top-single-project-section">
        <div class="content-container-static">
            <h5 class="category-headers" data-aos="fade-right" data-aos-duration="500" data-aos-delay="300">{{'_PROJECT_HEADER_CATEGORY_MAIN_' | trans}}</h5>
            <h3 class="title-single-project" data-aos="fade-right" data-aos-duration="500" data-aos-delay="600">{{'_POWERBODY_MAIN_TITLE_' | trans}}</h3>
            <div class="info-description-floats" data-aos="fade-right" data-aos-duration="500" data-aos-delay="900">
