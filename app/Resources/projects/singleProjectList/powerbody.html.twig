        <div class="content-container-static">
            <h3 data-aos="fade-right" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500"><span>{{'_POWERBODY_RESPONSIVE_SHOWCASE_TITLE_' | trans}}</span><br>{{'_POWERBODY_RESPONSIVE_SHOWCASE_SUBTITLE_' | trans}}</h3>
