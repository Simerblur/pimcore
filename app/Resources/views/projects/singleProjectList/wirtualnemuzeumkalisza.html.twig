{% extends 'layout/base.html.twig' %}
{% block title %}{{'_PROJECTMUZEUMKALISZA_META_TITLE_' | trans}}{% endblock %}
{% block keywords %}{{'_PROJECTMUZEUMKALISZA_META_KEYWORDS_' | trans}}{% endblock %}
{% block description %}{{'_PROJECTMUZEUMKALISZA_META_DESCRIPTION_' | trans}}{% endblock %}
{% block body %}
    <div id="kaliszmuseum" class="main-content-wrapper jsScrollToSection">
        <!-- section top  -->
        <section class="top-single-project-section">
            <div class="content-container-static">
                <h5 class="category-headers" data-aos="fade-right" data-aos-duration="500" data-aos-delay="300">{{'_CITY_SITE_' | trans}}</h5>
                <h3 class="title-single-project" data-aos="fade-right" data-aos-duration="500" data-aos-delay="600">{{'_WMK_PROJECT_MAIN_TITLE_' | trans}}</h3>
                <div class="info-description-floats" data-aos="fade-right" data-aos-duration="500" data-aos-delay="900">
                    <div class="info-description-floats-left">
                        <h5 class="category-headers inf-url-scope">{{'_INFO_SINGLE_PROJECT_' | trans}}</h5>
                        <p class="main-paragraph">{{'_INFO_DESCRIPTION_SINGLE_PROJECT_WMK_' | trans}}</p>
                    </div>
                    <div class="info-description-floats-right">
                        <div>
                            <h5 class="category-headers inf-url-scope">{{'_WEBSITE_SINGLE_PROJECT_' | trans}}:</h5>
                            <a href="http://www.wmf.kalisz.pl/" target="_blank" rel="nofollow">wmf.kalisz.pl</a>
                        </div>
                        <div>
                            <h5 class="category-headers inf-url-scope">{{'_SCOPE_SINGLE_PROJECT_' | trans}}:</h5>
                            <p class="main-paragraph category-paragraph">Visual design, UX design, Development</p>
                        </div>
                    </div>
                    <div class="info-description-floats-right platform-image-container">
                        <div>
                            <h5 class="category-headers inf-url-scope">{{'_PLATFORM_' | trans}}:</h5>
                            <div class="platform-image">
                                <img src="/images/zend.png" alt="{{'_WMK_PROJECT_IMAGE_1_ALT_' | trans}}"/>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="both-clear"></div>
                <img data-aos="fade-left" data-aos-duration="600" data-aos-delay="1300" src="/images/projects/wirtualnemuzeumkalisza/item1.png" id="item-1-wmk" alt="{{'_WMK_PROJECT_IMAGE_2_ALT_' | trans}}">
                <img data-aos="fade-left" data-aos-duration="700" data-aos-delay="1500" src="/images/projects/wirtualnemuzeumkalisza/item2.png" id="item-2-wmk" alt="{{'_WMK_PROJECT_IMAGE_3_ALT_' | trans}}">
                <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="1100" src="/images/projects/wirtualnemuzeumkalisza/iMac.png" id="imac-kalisz" alt="{{'_WMK_PROJECT_IMAGE_4_ALT_' | trans}}">
            </div>
        </section>
        <!-- top second section -->
        <section class="top-second-section">
            <img id="timeline-wmk" data-aos="fade-right" data-aos-duration="500" data-aos-delay="400" src="/images/projects/wirtualnemuzeumkalisza/timelinemuseum.png" alt="{{'_WMK_PROJECT_IMAGE_5_ALT_' | trans}}">
            <div class="content-container-static">
                <h3>{{'_MORE_THAN_1300_PICTURES_IN_GALLERY_' | trans}}</h3>
                <img id="photo-1-wmk" data-aos="fade-right" data-aos-duration="500" data-aos-delay="700" src="/images/projects/wirtualnemuzeumkalisza/photo1.png" alt="{{'_WMK_PROJECT_IMAGE_6_ALT_' | trans}}">
                <img id="photo-2-wmk" data-aos="fade-right" data-aos-duration="600" data-aos-delay="800" src="/images/projects/wirtualnemuzeumkalisza/photo2.png" alt="{{'_WMK_PROJECT_IMAGE_7_ALT_' | trans}}">
                <img id="photo-3-wmk" data-aos="fade-right" data-aos-duration="700" data-aos-delay="900" src="/images/projects/wirtualnemuzeumkalisza/photo3.png" alt="{{'_WMK_PROJECT_IMAGE_8_ALT_' | trans}}">
                <img id="photo-4-wmk" data-aos="fade-right" data-aos-duration="800" data-aos-delay="1000" src="/images/projects/wirtualnemuzeumkalisza/photo4.png" alt="{{'_WMK_PROJECT_IMAGE_9_ALT_' | trans}}">
                <img id="photo-5-wmk" data-aos="fade-right" data-aos-duration="900" data-aos-delay="1100" src="/images/projects/wirtualnemuzeumkalisza/photo5.png" alt="{{'_WMK_PROJECT_IMAGE_10_ALT_' | trans}}">
                <img id="photo-6-wmk" data-aos="fade-right" data-aos-duration="1000" data-aos-delay="1200" src="/images/projects/wirtualnemuzeumkalisza/photo6.png" alt="{{'_WMK_PROJECT_IMAGE_11_ALT_' | trans}}">
                <div class="both-clear"></div>
            </div>
        </section>
        <!-- responsive showcase -->
        <section class="responsive-showcase-section">
            <div class="content-container-static">
                <h3 data-aos="fade-right" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500"><span>{{'_VIRTUAL_TOUR_' | trans}}</span><br>{{'_JUST_FOR_YOU_' | trans}}</h3>
                <p class="main-paragraph" data-aos="fade-right" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="500">{{'_SHOWCASE_DESCRIPTION_WMK_' | trans}}</p>
                <img id="item-3-wmk" data-aos="fade-right" data-aos-duration="800" data-aos-delay="1000" src="/images/projects/wirtualnemuzeumkalisza/mainphoto.png" alt="{{'_WMK_PROJECT_IMAGE_12_ALT_' | trans}}">
                <img id="item-4-wmk" data-aos="fade-left" data-aos-duration="900" data-aos-delay="1100" src="/images/projects/wirtualnemuzeumkalisza/item3.png" alt="{{'_WMK_PROJECT_IMAGE_13_ALT_' | trans}}">
                <img id="item-5-wmk" data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1200" src="/images/projects/wirtualnemuzeumkalisza/item4.png" alt="{{'_WMK_PROJECT_IMAGE_14_ALT_' | trans}}">
                <img id="item-6-wmk" data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1200" src="/images/projects/wirtualnemuzeumkalisza/item5.png" alt="{{'_WMK_PROJECT_IMAGE_15_ALT_' | trans}}">
            </div>
        </section>
        <!-- about the technology -->
        <section class="about-technology-section">
            <div class="content-container-static">
                <h3 data-aos="fade-up">{{'_ABOUT_THE_TECHNOLOGY_' | trans}}</h3>
                <div class="technology-items-container" data-aos="zoom-in" data-aos-duration="600" data-aos-delay="300">
                    <div class="it-item" id="zend">Zend</div> 
                    <div class="it-item" id="php7">php7</div>
                    <div class="it-item" id="jquery">jQuery</div>
                    <div class="it-item" id="js">JS</div>
                    <div class="it-item" id="mysql">MySql</div>
                    <div class="it-item" id="html5">HTML5</div>
                    <div class="it-item" id="css3">CSS3</div>
                </div>
                <div class="both-clear"></div>
            </div>
        </section>
        <!-- overwiev -->
        <section class="overview-section">
            <div class="content-container-static">
                <h3 data-aos="fade-up">{{'_A_LOOK_@_THE_' | trans}}<br><span>{{'_WHOLE_' | trans}}</span></h3>
                <p class="main-paragraph" id="desc-paragraph" data-aos="fade-up">{{'_OVERVIEW_DESCRIPTION_WMK_' | trans}}</p>
                <div class="both-clear"></div>
                <div class="overview-description">
                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.01</h5>
                        <h6>{{'_WMK_MAIN_PAGE_' | trans}}<br><span>{{'_WMK_HOME_DIR_' | trans}}</span></h6>
                    </div>
                    <div class="overview-right" data-aos="fade-left">
                        <img class="whole-site-image" src="/images/projects/wirtualnemuzeumkalisza/Overview1.png" alt="{{'_WMK_PROJECT_IMAGE_16_ALT_' | trans}}Wirtualne Muzeum Kalisza">
                    </div>
                    <div class="both-clear"></div>

                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.02</h5>
                        <h6>{{'_WMK_ABOUT_' | trans}}<br><span>&amp; {{'_WMK_OUR_NEWS_' | trans}}</span></h6>
                    </div>
                    <div class="overview-right">
                        <div class="cat-presentation">
                            <img src="/images/projects/wirtualnemuzeumkalisza/Overview2.png" alt="{{'_WMK_PROJECT_IMAGE_17_ALT_' | trans}}Wirtualne Muzeum Kalisza" data-aos="fade-left" data-aos-anchor=".cat-presentation">
                            <img src="/images/projects/wirtualnemuzeumkalisza/Overview3.png" alt="{{'_WMK_PROJECT_IMAGE_18_ALT_' | trans}}Wirtualne Muzeum Kalisza" data-aos="fade-left" data-aos-anchor=".cat-presentation" data-aos-delay="300">
                        </div>
                        <div class="both-clear"></div>
                    </div>
                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.03</h5>
                        <h6>{{'_WMK_GALLERY_' | trans}}<br><span>&amp; {{'_WMK_VIEW_IMAGE_' | trans}}</span></h6>
                    </div>
                    <div class="overview-right">
                        <div class="cat-presentation second">
                            <img src="/images/projects/wirtualnemuzeumkalisza/Overview4.png" alt="{{'_WMK_PROJECT_IMAGE_19_ALT_' | trans}}Wirtualne Muzeum Kalisza" data-aos="fade-left" data-aos-anchor=".cat-presentation">
                            <img src="/images/projects/wirtualnemuzeumkalisza/Overview5.png" alt="{{'_WMK_PROJECT_IMAGE_20_ALT_' | trans}}Wirtualne Muzeum Kalisza" data-aos="fade-left" data-aos-anchor=".cat-presentation.sec" data-aos-delay="300">
                        </div>
                        <div class="both-clear"></div>
                    </div>
                    <div class="overview-left" data-aos="fade-right">
                        <h5 class="number-with-border">.04</h5>
                        <h6>{{'_WMK_ADD_PHOTO_' | trans}}<br><span>&amp; {{'_WMK_CONTACT_' | trans}}</span></h6>
                    </div>
                    <div class="overview-right">
                        <div class="cat-presentation third">
                            <img src="/images/projects/wirtualnemuzeumkalisza/Overview6.png" alt="{{'_WMK_PROJECT_IMAGE_21_ALT_' | trans}}Wirtualne Muzeum Kalisza" data-aos="fade-left" data-aos-anchor=".cat-presentation.third">
                            <img src="/images/projects/wirtualnemuzeumkalisza/Overview7.png" alt="{{'_WMK_PROJECT_IMAGE_22_ALT_' | trans}}Wirtualne Muzeum Kalisza" data-aos="fade-left" data-aos-anchor=".cat-presentation" data-aos-delay="300">
                        </div>
                        <div class="both-clear"></div>
                    </div>
                    <div class="both-clear"></div>
                </div>
                <div class="both-clear"></div>  
            </div>
        </section>
        <!-- our team -->
        <section class="our-crazy-team">
            <div class="content-container-static">
                <h3 data-aos="fade-up">{{'_OUR_CRAZY_TEAM_' | trans}}</h3>
                <div id="project-manager-container" data-aos="fade-down" data-aos-duration="500" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/rafalprojectmanager.png" alt="Rafał" class="person-default">
                        <img src="/images/persons/custom-shapes/projectmanagerhover.png" alt="Rafał" class="person-hover">
                        <div class="person-desc">
                            <h6>.01</h6>
                            <p>RAFAŁ<br><span>{{'_PROJECT_MANAGER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
                <div id="ui-ux-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="200" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/izauxkolor.png" alt="Iza" class="person-default">
                        <img src="/images/persons/custom-shapes/uiuxhover.png" alt="Iza" class="person-hover">
                        <div class="person-desc">
                            <h6>.02</h6>
                            <p>IZA<br><span>{{'_UIUX_DESIGNER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
                <div id="graphic-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="400" data-aos-anchor=".our-crazy-team">
                    <div class="single-person">
                        <img src="/images/persons/custom-shapes/michalkolor.png" alt="Michał" class="person-default">
                        <img src="/images/persons/custom-shapes/developerhover.png" alt="Michał" class="person-hover">
                        <div class="person-desc">
                            <h6>.03</h6>
                            <p>MICHAŁ<br><span>{{'_DEVELOPER_' | trans}}</span></p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- next/prev project -->
        <section class="next-prev-section">
            <div class="content-container-static">
                <a href="{{ path('polishsoulsproject') }}" id="prev-btn"><span></span><p>{{'_PREVIOUS_' | trans}}</p></a>
                <h6><a href="{{ path('projectslist') }}">{{'_SEE_ALL_PROJECTS_' | trans}}</a></h6>
                <a href="{{ path('easybathroomsproject') }}" id="next-btn"><p>{{'_NEXT_' | trans}}</p><span></span></a>
            </div>
        </section>
    </div>
    <!-- animation on scroll scritp -->
    <script src="/scripts/aos/aos.js"></script>
    <script>
        AOS.init({
            easing: 'ease-out-back',
            once: true
        });
    </script>
    <script src="{{ asset('/scripts/Advox/scrollToSection.js') }}"></script>
{% endblock %}


