{% extends 'layout/base.html.twig' %}
{% block title %}{{'_PROJECTAMBIGANTE_META_TITLE_' | trans}}{% endblock %}
{% block keywords %}{{'_PROJECTAMBIGANTE_META_KEYWORDS_' | trans}}{% endblock %}
{% block description %}{{'_PROJECTAMBIGANTE_META_DESCRIPTION_' | trans}}{% endblock %}
{% block body %}
<div id="ambigante" class="main-content-wrapper jsScrollToSection">
    <!-- section top  -->
    <section class="top-single-project-section">
        <div class="content-container-static">
            <h5 class="category-headers" data-aos="fade-right" data-aos-duration="500" data-aos-delay="300">{{'_PROJECT_HEADER_CATEGORY_MAIN_' | trans}}</h5>
            <h3 class="title-single-project" data-aos="fade-right" data-aos-duration="500" data-aos-delay="600">{{'_AMBIGANTE_PROJECT_MAIN_TITLE_' | trans}}</h3>
            <div class="info-description-floats" data-aos="fade-right" data-aos-duration="500" data-aos-delay="900">
                <div class="info-description-floats-left">
                    <h5 class="category-headers inf-url-scope">{{'_INFO_SINGLE_PROJECT_' | trans}}</h5>
                    <p class="main-paragraph">{{'_INFO_DESCRIPTION_SINGLE_PROJECT_AMBIGANTE_' | trans}}</p>
                </div>
                <div class="info-description-floats-right">
                    <div>
                        <h5 class="category-headers inf-url-scope">{{'_WEBSITE_SINGLE_PROJECT_' | trans}}:</h5>
                        <a href="http://ambigante.com/" target="_blank" rel="nofollow">ambigante.com</a>
                    </div>
                    <div>
                        <h5 class="category-headers inf-url-scope">{{'_SCOPE_SINGLE_PROJECT_' | trans}}:</h5>
                        <p class="main-paragraph category-paragraph">Visual design, UX design, Development</p>
                    </div>
                </div>
                <div class="info-description-floats-right platform-image-container">
                    <div>
                        <h5 class="category-headers inf-url-scope">{{'_PLATFORM_' | trans}}:</h5>
                        <div class="platform-image">
                            <img src="{{ asset('/images/svg/magento_project_view.svg') }}" alt="{{'_AMBIGANTE_PROJECT_IMAGE_1_ALT_' | trans}}"/>
                        </div>
                    </div>
                </div>
            </div>
            <div class="both-clear"></div>
            <img data-aos="fade-right" data-aos-duration="500" data-aos-delay="1600" src="{{ asset('/images/projects/ambigante/palto.png') }}" id="coat-ambigante" alt="{{'_AMBIGANTE_PROJECT_IMAGE_2_ALT_' | trans}}">
            <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="1300" src="{{ asset('/images/projects/ambigante/pomadki.png') }}" id="lipsticks" alt="{{'_AMBIGANTE_PROJECT_IMAGE_3_ALT_' | trans}}">
            <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="1600" src="{{ asset('/images/projects/ambigante/spodnie.png') }}" id="jeans" alt="{{'_AMBIGANTE_PROJECT_IMAGE_4_ALT_' | trans}}">
            <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="1600" src="{{ asset('/images/projects/ambigante/szpilki.png') }}" id="high-heels" alt="{{'_AMBIGANTE_PROJECT_IMAGE_5_ALT_' | trans}}">
            <img data-aos="fade-up" data-aos-duration="800" data-aos-delay="1600" src="{{ asset('/images/projects/ambigante/macpro-ambigante.png') }}" id="mac-ambigante" alt="{{'_AMBIGANTE_PROJECT_IMAGE_6_ALT_' | trans}}">
        </div>
    </section>
    <!-- top second section -->
    <section class="top-second-section">
        <div class="content-container-static">
            <h3>{{'_APPROACHABLE_CATEGORY_VIEW' | trans}}</h3>
            <div class="top-seceond-image-wrapper-ambigante">
                <img data-aos="fade-right" data-aos-duration="500" src="../images/projects/ambigante/slidesleft.png" alt="{{'_AMBIGANTE_PROJECT_IMAGE_7_ALT_' | trans}}">
                <img data-aos="fade-left" data-aos-duration="500" data-aos-delay="300" src="../images/projects/ambigante/slidesright.png" alt="{{'_AMBIGANTE_PROJECT_IMAGE_8_ALT_' | trans}}">
            </div>
            <div class="both-clear"></div>
        </div>
    </section>
    <!-- responsive showcase -->
    <section class="responsive-showcase-section">
        <div class="content-container-static">
            <h3 data-aos="fade-right" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500">{{'_AMBIGANTE_RESPONSIVE_SHOWCASE_TITLE_' | trans}}<br><span>{{'_AMBIGANTE_RESPONSIVE_SHOWCASE_SUBTITLE_' | trans | raw}}</span></h3>
            <p class="main-paragraph" data-aos="fade-right" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="500">{{'_RESPONSIVE_SHOWCASE_DESCRIPTION_AMBIGANTE_' | trans}}</p>
            <img id="handbag" src="../images/projects/ambigante/torebka.png" alt="{{'_AMBIGANTE_PROJECT_IMAGE_9_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="700">
            <img id="glasses" src="../images/projects/ambigante/glasses.png" alt="{{'_AMBIGANTE_PROJECT_IMAGE_10_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="900">
            <img id="high-heels2" src="../images/projects/ambigante/pantofle.png" alt="{{'_AMBIGANTE_PROJECT_IMAGE_11_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="1100">
            <img id="laptops" src="../images/projects/ambigante/laptops.png" alt="{{'_AMBIGANTE_PROJECT_IMAGE_12_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".responsive-showcase-section" data-aos-duration="500" data-aos-delay="1300">
        </div>
    </section>
    <!-- about the technology -->
    <section class="about-technology-section">
        <div class="content-container-static">
            <h3 data-aos="fade-up">{{'_ABOUT_THE_TECHNOLOGY_' | trans}}</h3>
            <div class="technology-items-container" data-aos="zoom-in" data-aos-duration="600" data-aos-delay="300">
                <div class="it-item" id="magento">Magento</div> 
                <div class="it-item" id="php7">php7</div>
                <div class="it-item" id="js">JS</div>
                <div class="it-item" id="jquery">jQuery</div>
                <div class="it-item" id="mysql">MySql</div>
                <div class="it-item" id="html5">HTML5</div>
                <div class="it-item" id="css3">CSS3</div>
            </div>
            <div class="both-clear"></div>
        </div>
    </section>
    <!-- overwiev -->
    <section class="overview-section">
        <div class="content-container-static">
            <h3 data-aos="fade-up">{{'_A_LOOK_@_THE_' | trans}}<br><span>{{'_WHOLE_' | trans}}</span></h3>
            <p class="main-paragraph" id="desc-paragraph" data-aos="fade-up">{{'_OVERVIEW_DESCRIPTION_AMBIGANTE_' | trans}}</p>
            <div class="both-clear"></div>
            <div class="overview-description">
                <div class="overview-left" data-aos="fade-right">
                    <h5 class="number-with-border">.01</h5>
                    <h6>{{'_MAIN_PAGE_' | trans}}<br><span>{{'_HOME_DIR' | trans}}</span></h6>
                </div>
                <div class="overview-right" data-aos="fade-left">
                    <img class="whole-site-image" src="../images/projects/ambigante/overview-1.jpg" alt="{{'_AMBIGANTE_PROJECT_IMAGE_13_ALT_' | trans}}">
                </div>
                <div class="both-clear"></div>
                <div class="overview-left" data-aos="fade-right">
                    <h5 class="number-with-border">.02</h5>
                    <h6>{{'_CATEGORY_' | trans}}<br><span>&amp; {{'_PRODUCT_PAGE_' | trans}}</span></h6>
                </div>
                <div class="overview-right">
                    <div class="cat-presentation">
                        <img src="../images/projects/ambigante/overview-2.jpg" alt="{{'_AMBIGANTE_PROJECT_IMAGE_14_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation">
                        <img src="../images/projects/ambigante/overview-3.jpg" alt="{{'_AMBIGANTE_PROJECT_IMAGE_15_ALT_' | trans}}" data-aos="fade-left" data-aos-anchor=".cat-presentation" data-aos-delay="300">
                    </div>
                    <div class="both-clear"></div>
                </div>
                <div class="both-clear"></div>
            </div>
            <div class="both-clear"></div>  
        </div>
    </section>
    <!-- our team -->
    <section class="our-crazy-team">
        <div class="content-container-static">
            <h3 data-aos="fade-up">{{'_OUR_CRAZY_TEAM_' | trans}}</h3>
            <div id="project-manager-container" data-aos="fade-down" data-aos-duration="500" data-aos-anchor=".our-crazy-team">
                <div class="single-person">
                    <img src="../images/persons/custom-shapes/rafalprojectmanager.png" alt="Iza" class="person-default">
                    <img src="../images/persons/custom-shapes/projectmanagerhover.png" alt="Iza" class="person-hover">
                    <div class="person-desc">
                        <h6>.01</h6>
                        <p>RAFA≈Å<br><span>{{'_PROJECT_MANAGER_' | trans}}</span></p>
                    </div>
                </div>
            </div>
            <div id="ui-ux-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="200" data-aos-anchor=".our-crazy-team">
                <div class="single-person">
                    <img src="../images/persons/custom-shapes/izauxkolor.png" alt="Iza" class="person-default">
                    <img src="../images/persons/custom-shapes/uiuxhover.png" alt="Iza" class="person-hover">
                    <div class="person-desc">
                        <h6>.02</h6>
                        <p>IZA<br><span>{{'_UIUX_DESIGNER_' | trans}}</span></p>
                    </div>
                </div>
            </div>
            <div id="graphic-container" data-aos="fade-down" data-aos-duration="500" data-aos-delay="400" data-aos-anchor=".our-crazy-team">
                <div class="single-person">
                    <img src="../images/persons/custom-shapes/marcindeveloper.png" alt="Iza" class="person-default">
                    <img src="../images/persons/custom-shapes/graphicdesignerhover.png" alt="Iza" class="person-hover">
                    <div class="person-desc">
                        <h6>.03</h6>
                        <p>MARCIN<br><span>{{'_DEVELOPER_' | trans}}</span></p>
                    </div>
                </div>
            </div>
        </div>
    </section>
            <!-- next/prev project -->
    <section class="next-prev-section">
        <div class="content-container-static">
            <a href="{{ path('mmprimeproject') }}" id="prev-btn"><span></span><p>{{'_PREVIOUS_' | trans}}</p></a>
            <h6><a href="{{ path('projectslist') }}">{{'_SEE_ALL_PROJECTS_' | trans}}</a></h6>
            <a href="{{ path('virtualofficeproject') }}" id="next-btn"><p>{{'_NEXT_' | trans}}</p><span></span></a>
        </div>
    </section>
</div>
{% endblock %}
{% block jsBottom %}
    <script src="{{ asset('/scripts/aos/aos.js') }}"></script>
    <script>
        AOS.init({
            easing: 'ease-out-back',
            once: true
        });
    </script>
    <script src="{{ asset('/scripts/Advox/scrollToSection.js') }}"></script>
{% endblock %}
