    <script>

        var slideTimeout;
        $(document).ready(function () {

            $('#fullpage').fullpage({
                anchors: ['{{'_UXS_ANCHOR_LINK_1_' | trans}}', '{{'_UXS_ANCHOR_LINK_2_' | trans}}', '{{'_UXS_ANCHOR_LINK_3_' | trans}}', '{{'_UXS_ANCHOR_LINK_4_' | trans}}', '{{'_UXS_ANCHOR_LINK_5_' | trans}}', '{{'_UXS_ANCHOR_LINK_6_' | trans}}', '{{'_UXS_ANCHOR_LINK_7_' | trans}}', '{{'_UXS_ANCHOR_LINK_8_' | trans}}'],
                paddingBottom: '60px',
                navigation: true,
                navigationPosition: 'right',
                fixedElements: '#header,.service-navigation-footer-mobile',
                scrollOverflow: true,
                controlArrows: false,
                afterSlideLoad: function (anchorLink, index, slideAnchor, slideIndex) {
                    paginationHiglighting();
                },
                afterLoad: function (anchorLink, index) {

                    if (index === $('#fullpage .section').length) {
                        $('.goUp').show();
                        $('.goDeeper,.downArrowNavigation').hide();
                    } else {
                        $('.downArrowNavigation').show();
                    }

                    if (index == 1) {
                        $('.goUp,.upArrowNavigation').hide();
                        $('.goDeeper').show();
                    } else {
                        $('.upArrowNavigation').show();
                    }
                    
                    
                    switch (index)
                    {
                        case 1:
                            darkPagination();
                        break;
                        case 2:
                            darkPagination();
                        break;
                        case 3:
                            darkPagination();
                        break;
                        case 4:
                            darkPagination();
                        break;
                        case 5:
                            whitePagination();
                            break;
                        case 6:
                            darkPagination();
                            break;
                        case 7:
                            darkPagination();
                            break;
                        case 8:
                            whitePagination();
                            break;
                    }
                    
                }
            });

            $("#fp-nav").prepend("<span class='upArrowNavigation'></span>");
            $("#fp-nav").append("<span class='downArrowNavigation'></span>");

            $('.upArrowNavigation').click(function () {
                $.fn.fullpage.moveSectionUp();
            });

            $('.goDeeper,.downArrowNavigation').click(function () {
                $.fn.fullpage.moveSectionDown();
            });

            paginationHiglighting();
        });
        
        function darkPagination() {
             $('#fp-nav ul li a span').css('background-color','rgba(60, 60, 61, 0.3) !important').css('transition','background-color 0.5s');
             $('#fp-nav ul li a.active span').css('background-color','rgba(60, 60, 61, 0.7) !important').css('transition','background-color 0.5s');
        }
        
        function whitePagination() {
            $('#fp-nav ul li a span').css('background-color','rgba(249, 249, 249, 0.3) !important').css('transition','background-color 0.5s');
            $('#fp-nav ul li a.active span').css('background-color','rgba(249, 249, 249, 0.7) !important').css('transition','background-color 0.5s');
        }

        function paginationHiglighting() {
            var dataSlideName = $(".fp-slide.fp-table.active").data('slidename');

            $('.custom-pagination-slide').css('color', 'black').css('font-size', '13px');
            $('.custom-pagination-slide[data-slidename="' + dataSlideName + '"]').css('color', '#00a5db').css('font-size', '19px');
        }

        $(".slide-left-custom").click(function () {
            $.fn.fullpage.moveSlideLeft();
            paginationHiglighting();
        });

        $(".slide-right-custom").click(function () {
            $.fn.fullpage.moveSlideRight();
            paginationHiglighting();
        });

        $(".custom-pagination-slide").click(function () {

            var dataSlideId = $(this).data('slideid');

            $.fn.fullpage.moveTo('{{'_UXS_ANCHOR_LINK_8_' | trans}}', dataSlideId);
            
            paginationHiglighting();

        });
    </script>
